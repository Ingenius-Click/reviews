{
    "name": "ingenius/reviews",
    "description": "Ingenius Reviews Package",
    "type": "library",
    "license": "MIT",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Ingenius Team",
            "email": "info@ingeniusclick.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ingenius/core": "*",
        "ingenius/products": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ingenius\\Reviews\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ingenius\\Reviews\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ingenius\\Reviews\\Providers\\ReviewsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
